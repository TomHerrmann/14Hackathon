{
  "name": "Day Tab",
  "version": "1.0",
  "description": "Opens a new tab with everything you need for your day.",
  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["activeTab", "geolocation"],
  "icons": {
    "128": "icon128.png"
  }
}
